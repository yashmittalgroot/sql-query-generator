# SQL Query Generator - Environment Configuration Template
# Copy this file to .env and fill in your actual values

# REQUIRED: Google Gemini AI Configuration
GEMINI_API_KEY=your_gemini_api_key_here

# REQUIRED: PostgreSQL Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=your_database_name
DB_USER=your_username
DB_PASSWORD=your_password

# OPTIONAL: MCP Server Configuration
MCP_SERVER_URL=http://localhost:8080
MCP_SERVER_NAME=postgres-mcp-server

# Application Settings
DEBUG=True
LOG_LEVEL=INFO

# Performance Settings (Optional)
MAX_SCHEMA_TABLES=50
SCHEMA_CACHE_TIMEOUT=300
